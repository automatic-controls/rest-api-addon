{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "DeleteOperator",
  "description": "Delete an operator from the system.",
  "type": "object",
  "properties": {
    "operator": {
      "anyOf": [
        {
          "type": "string",
          "description": "The username of the operator to delete.",
          "minLength": 1
        },
        {
          "type": "array",
          "items": {
            "type": "string",
            "minLength": 1
          },
          "description": "A list of operator usernames to delete.",
          "minItems": 1,
          "uniqueItems": true
        }
      ]
    },
    "contextDBID": {
      "type": "integer",
      "description": "This parameter is ignored."
    }
  },
  "required": [
    "operator"
  ],
  "additionalProperties": false
}